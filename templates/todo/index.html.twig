{% extends 'Template.html.twig' %}
{% block title %}
    Todo
{% endblock %}

{% block body %}
{{ include('fragments/fragment.html.twig') }}
{% block pageTitle %}
    <h1>To-do</h1>
{% endblock%}
<ul class="list-group">
    {% for cle,element in app.session.get('todos') %}
        <li class="list-group-item">{{ cle }} : {{ element }}</li>
    {% endfor %}
</ul>
{% endblock %}